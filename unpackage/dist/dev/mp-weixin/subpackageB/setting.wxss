
.settings-container {
  padding: 20rpx;
  width: 100%;
}
.header {
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
}
.title {
  font-size: 36rpx;
  font-weight: bold;
}
.settings-list {
  margin-top: 20rpx;
}
.settings-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #f5f5f5;
  font-size: 32rpx;
}

