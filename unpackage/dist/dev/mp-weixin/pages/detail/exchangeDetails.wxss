
page {
	display: flex;
	flex-direction: column;
	box-sizing: border-box;
	background-color: #efeff4;
	min-height: 100%;
	height: auto;
}
view {
	font-size: 28rpx;
	line-height: inherit;
}
.content {
	padding: 0;
}
.content-content {
	justify-content: center;
	align-items: center;
	text-align: left;
}
.input-view {
	align-items: center;
	justify-content: center;
	width: 100%;
	display: flex;
	background-color: #e7e7e7;
	height: 30px;
	border-radius: 15px;
	padding: 0 4%;
	flex-wrap: nowrap;
	margin: 7px 10rpx;
	line-height: 30px;
	background: #f5f5f5;
}
.input-view .input {
	height: 30px;
	line-height: 30px;
	width: 94%;
	padding: 0 3%;
}

