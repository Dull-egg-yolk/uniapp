
.custom-tabbar.data-v-6d2573a3 {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 999;
}
.tabbar.data-v-6d2573a3 {
  display: flex;
  height: 120rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% auto;
  /* border-top: 1rpx solid #f0f0f0; */
}
#tabbar0.data-v-6d2573a3 {
  border-top-right-radius:20rpx;
}
.tabbar-item.data-v-6d2573a3 {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.icon.data-v-6d2573a3 {
  width: 60rpx;
  height: 60rpx;
  color:rgba(190,190,190,1);
  margin-top: 14rpx;
}
.text.data-v-6d2573a3 {
  font-size: 24rpx;
  margin-top: 4rpx;
  color: rgba(190,190,190,1);
}
.active .text.data-v-6d2573a3 {

  color: rgba(0,0,0,1);/* 选中状态颜色 */
}
.ctive .icon.data-v-6d2573a3{
  color:rgba(79,79,79,1);
}

/* 安全区域适配 */
.safe-area.data-v-6d2573a3 {
  height: env(safe-area-inset-bottom);
  background: #ffffff;
}
.tabbar-item.data-v-6d2573a3 {
  transition: -webkit-transform 0.2s;
  transition: transform 0.2s;
  transition: transform 0.2s, -webkit-transform 0.2s;
}
.tabbar-item.data-v-6d2573a3:active {
  -webkit-transform: scale(0.9);
          transform: scale(0.9);
}
